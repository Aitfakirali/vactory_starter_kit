{% if menu_links is not empty %}
    {{ attach_library('vactory_anchor/anchorLib') }}
    <div class='vactory-ancre'>
        <ul class="nav container">
            {% for item in menu_links %}
                <li class="nav-item">
                    <a class="nav-link anchor-link" href="#{{ item.id }}" id="link-{{ item.id }}">{{ item.label }}</a>
                </li>
            {% endfor %}
        </ul>
    </div>
    <div class="ancre-custom-block">
        {% if custom_block is not empty %}
            {{ vactory_render('block', custom_block) }}
        {% endif %}
    </div>
{% endif %}
