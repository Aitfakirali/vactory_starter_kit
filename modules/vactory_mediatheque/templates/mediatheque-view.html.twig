{%
  set classes = [
  dom_id ? 'js-view-dom-id-' ~ dom_id,
  'container'
]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {{ title }}
  {{ title_suffix }}

  {% if header %}
    <header>
      {{ header }}
    </header>
  {% endif %}
  {{ exposed }}
  <div class="mb-3">
    {{ vactory_render('block', 'year_block_filter') }}
  </div>
  {{ attachment_before }}

  {% if rows -%}
      {{ rows }}
  {% elseif empty -%}
    {{ empty }}
  {% endif %}
  {{ pager }}

  {{ attachment_after }}
  {{ more }}

  {% if footer %}
    <footer>
      {{ footer }}
    </footer>
  {% endif %}

  {{ feed_icons }}
</div>

